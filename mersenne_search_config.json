{
  "search_configuration": {
    "search_strategy": "pattern_guided",
    "num_predictions": 1,
    "max_workers": "auto",
    "batch_size": 1000,
    "early_termination": true,
    "progress_reporting": true
  },
  
  "pattern_analysis": {
    "exponential_model_weight": 0.7,
    "polynomial_model_weight": 0.2,
    "gap_analysis_weight": 0.1,
    "confidence_threshold": 0.8
  },
  
  "optimization_settings": {
    "use_miller_rabin": true,
    "use_early_factor_check": true,
    "use_modular_optimizations": true,
    "use_parallel_processing": true,
    "memory_optimization": true
  },
  
  "search_ranges": {
    "range_1": {
      "description": "Focused slice for quick Prime95 feedback",
      "start": 85000000,
      "end": 85200000,
      "priority": "high"
    },
    "range_2": null,
    "range_3": null
  },
  
  "prime95_integration": {
    "enabled": true,
    "prime95_dir": "C:/Prime95",
    "worktodo_path": "C:/Prime95/worktodo.txt",
    "mode": "LL",
    "append": true,
    "auto_launch": false,
    "use_for_ll": false,
    "results_path": "C:/Prime95/results.txt"
  },
  
  "hardware_optimization": {
    "cpu_cores": "auto",
    "gpu_acceleration": false,
    "memory_limit_gb": 8,
    "disk_cache": true
  },
  
  "monitoring": {
    "save_progress": true,
    "progress_interval": 1000,
    "backup_results": true,
    "log_level": "info"
  },
  "cpp_core": {
    "enabled": false,
    "exe_path": "candidate_generator.exe"
  }
}
